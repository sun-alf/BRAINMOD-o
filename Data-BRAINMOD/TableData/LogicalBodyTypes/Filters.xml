			<VESTPOS op="btwn">2886, 2888</VESTPOS>  <!-- Shielded Vest -->

			<!-- ITEMS BELOW SHOULD BE MOVED BACK TO CamoLongsleeves SECTION WHEN PROPER ANIMATIONS ADDED -->
			<VESTPOS op="btwn">2701, 2707</VESTPOS>  <!-- Camo Uniforms, Zylon Combat Vest -->
			<VESTPOS op="btwn">2734, 2736</VESTPOS>  <!-- Bristol Vest & Jumper -->
