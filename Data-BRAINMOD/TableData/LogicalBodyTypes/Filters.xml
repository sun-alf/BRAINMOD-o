			<VESTPOS op="btwn">2829, 2834</VESTPOS>  <!-- Spectra Vest Field Ops/Desert Ops -->
			<VESTPOS op="btwn">2844, 2855</VESTPOS>  <!-- Twaron Vest Recon/Field Ops/Desert Ops -->
